{
    "name": "Notable",
    "version": "1.1",
    "description": "Sticky Notes and Notetaking Chrome Extension!",
    "permissions": [
      "activeTab",
      "tabs"
    ],      
    "browser_action": {
      "default_popup": "popup.html",
        "default_icon": {
        "48": "images/logo.png",
        "128": "images/logo128.png"
      }
    },
    "icons": {
      "48": "images/logo.png",
      "128": "images/logo128.png"
    }, 
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js":["js/p5.js", "draw.js"]
    }],
    "manifest_version": 2
  }